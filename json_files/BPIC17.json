{
  "name": "BPIC17",
  "version": "1.0.0",
  "nodes": [
    {
      "type": "Event",
      "constructed_by_node": "(entry:EventEntry)",
      "result": "(e:Event {timestamp:entry.timestamp, activity:entry.activity, lifecycle:entry.lifecycle})"
    },
    {
      "type": "Activity",
      "constructed_by_node": "(entry:EventEntry)",
      "result": "(a:Activity {activity:entry.activity, lifecycle:entry.lifecycle})",
      "observed": true
    },
    {
      "type": "Application",
      "constructed_by_node": "(entry:EventEntry {eventOrigin: 'Application'})",
      "result": "(a:Entity:Application {sysId: entry.case, loanGoal: entry.loanGoal, requestedAmount: entry.requestedAmount})",
      "corr": true,
      "df": true,
      "include_label_in_df": true,
      "merge_duplicate_df": true
    },
    {
      "type": "Workflow",
      "constructed_by_node": "(entry:EventEntry {eventOrigin: 'Workflow'})",
      "result": "(w:Entity:Workflow {sysId: entry.case})",
      "df": true,
      "include_label_in_df": true,
      "corr": true,
      "merge_duplicate_df": true
    },
    {
      "type": "Offer",
      "constructed_by_node": "(entry:EventEntry WHERE entry.eventOrigin = 'Offer' AND entry.eventId STARTS WITH 'Offer_')",
      "result": "(o:Entity:Offer {sysId: entry.eventId, case: entry.case, creditScore: entry.creditScore, firstWithdrawalAmount: entry.firstWithdrawalAmount, monthlyCost: entry.monthlyCost, numberOfTerms: entry.numberOfTerms, offeredAmount: entry.offeredAmount})",
      "df": true,
      "include_label_in_df": true,
      "corr": true,
      "merge_duplicate_df": true
    },

    {
      "type": "Offer",
      "constructed_by_node": "(entry:EventEntry WHERE entry.eventOrigin = 'Offer' AND entry.eventId STARTS WITH 'OfferState_')",
      "result": "(o:Entity:Offer {sysId: entry.offerId})",
      "df": true,
      "include_label_in_df": true,
      "corr": true,
      "merge_duplicate_df": true
    },

    {
      "type": "Resource",
      "constructed_by_node": "(entry:EventEntry)",
      "result": "(r:Entity:Resource {sysId: entry.resource})",
      "df": true,
      "include_label_in_df": true,
      "corr": true,
      "merge_duplicate_df": true
    },
    {
      "type": "CaseAWO",
      "constructed_by_node": "(entry:EventEntry)",
      "result": "(case_awo:Entity:CaseAWO {sysId: entry.case)}",
      "corr": false,
      "df": false
    },
    {
      "type": "CaseAO",
      "constructed_by_relation": "(o:Offer) - [:CASE_AO] -> (a:Application)",
      "result": "(case_ao:Entity:CaseAO {offerId: o.sysId, applicationId: a.sysId)",
      "model_reified_relations": true,
      "corr": true,
      "df": false
    },
    {
      "type": "CaseAW",
      "constructed_by_relation": "(w:Workflow) - [:CASE_AW] -> (a:Application)",
      "result": "(case_aw:Entity:CaseAW {workflowId: w.sysId, applicationId: a.sysId)",
      "model_reified_relations": true,
      "corr": true,
      "df": false
    },
    {
      "type": "CaseWO",
      "constructed_by_relation": "(o:Offer) - [:CASE_WO] -> (w:Workflow)",
      "result": "(case_wo:Entity:CaseWO {workflowId: w.sysId, offerId: o.sysId)",
      "model_reified_relations": true,
      "labels": [
        "CaseWO"
      ],
      "corr": true,
      "df": false
    }
  ],
  "relations": [
    {
      "type": "CASE_AO",
      "constructed_by_nodes": {
        "from_node": "(o:Offer {case: a.sysId})",
        "to_node": "(a: Application)"
      },
      "result": "(o) - [:CASE_AO] -> (a)"
    },
    {
      "type": "CASE_AW",
      "constructed_by_nodes": {
        "from_node": "(w: Workflow)",
        "to_node": "(a: Application {case: w.sysId})"
      },
      "result": "(w) - [:CASE_AW] -> (a)"
    },
    {
      "type": "CASE_WO",
      "constructed_by_nodes": {
        "from_node": "(o: Offer {case: w.sysId})",
        "to_node": "(w: Workflow)"
      },
      "result": "(o) - [:CASE_WO] -> (w)"
    }
  ]
}
